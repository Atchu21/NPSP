{
  "orgName": "NPSP - Enterprise Workspace",
  "edition": "Enterprise",
  "hasSampleData": "false",
  "features": [
    "EventLogFile", "AnalyticsAdminPerms", "EinsteinAnalyticsPlus", "SecurityEventEnabled"
  ],
  "settings": {
    "chatterSettings": {
      "enableChatter": true
    },
    "userManagementSettings": {
      "enableNewProfileUI": true
    },
    "apexSettings": {
      "enableDisableParallelApexTesting": true
    },
    "enhancedNotesSettings": {
      "enableEnhancedNotes": true
    },
    "lightningExperienceSettings": {
      "enableS1DesktopEnabled": true
    },
    "languageSettings": {
      "enableTranslationWorkbench": true
    }
  }
}